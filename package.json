{
  "name": "@mgp/rc-slider",
  "version": "0.0.3",
  "description": "Slider UI component for React",
  "keywords": [
    "react",
    "react-component",
    "react-slider",
    "slider"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mgp-dev/rc-slider.git"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "main": "dist/rc-slider.cjs.js",
  "module": "dist/rc-slider.esm.js",
  "umd:main": "dist/rc-slider.umd.js",
  "scripts": {
    "rollup": "rollup -c",
    "less": "lessc assets/index.less dist/index.css",
    "build": "yarn run less && yarn run rollup",
    "test": "jest",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "devDependencies": {
    "enzyme": "^2.8.0",
    "enzyme-to-json": "^1.5.1",
    "jest": "^20.0.0",
    "less": "^2.7.2",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-test-renderer": "^15.5.4",
    "rollup": "^0.49.3",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-typescript2": "^0.5.2",
    "rollup-plugin-uglify": "^2.0.1",
    "ts-jest": "^21.0.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "create-react-class": "^15.6.0",
    "dom-align": "^1.6.4",
    "prop-types": "^15.5.4",
    "rc-util": "^4.0.4",
    "shallowequal": "^1.0.1",
    "warning": "^3.0.0"
  },
  "bugs": {
    "url": "https://github.com/mgp-dev/rc-slider/issues"
  },
  "homepage": "https://github.com/mgp-dev/rc-slider#readme",
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "author": ""
}
