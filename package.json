{
  "name": "rc-slider",
  "version": "8.3.1",
  "description": "Slider UI component for React",
  "keywords": [
    "react",
    "react-component",
    "react-slider",
    "slider"
  ],
  "homepage": "https://react-component.github.io/slider/",
  "repository": {
    "type": "git",
    "url": "git@github.com:react-component/slider.git"
  },
  "bugs": {
    "url": "http://github.com/react-component/slider/issues"
  },
  "files": [
    "assets/*.css",
    "lib",
    "es",
    "dist"
  ],
  "license": "MIT",
  "main": "dist/how-long-till-lunch.cjs.js",
  "module": "dist/how-long-till-lunch.esm.js",
  "browser": "dist/how-long-till-lunch.umd.js",
  "scripts": {
    "build": "rollup -c",
    "less": "lessc assets/index.less dist/common.css",
    "test": "jest",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/*"
    ],
    "transform": {
      "\\.tsx?$": "./node_modules/rc-tools/scripts/jestPreprocessor.js",
      "\\.jsx?$": "./node_modules/rc-tools/scripts/jestPreprocessor.js"
    }
  },
  "devDependencies": {
    "enzyme": "^2.8.0",
    "enzyme-to-json": "^1.5.1",
    "jest": "^20.0.0",
    "less": "^2.7.2",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-test-renderer": "^15.5.4",
    "rollup": "^0.49.3",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-typescript2": "^0.5.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "create-react-class": "^15.6.0",
    "dom-align": "^1.6.4",
    "prop-types": "^15.5.4",
    "rc-util": "^4.0.4",
    "shallowequal": "^1.0.1",
    "warning": "^3.0.0"
  }
}
